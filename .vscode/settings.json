// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "client/app/*.js": true,
        "client/app/*.js.map": true,
        "**/__pycache__": true
    },
    "python.pythonPath": "~/anaconda/envs/cats/bin/python",
    "python.linting.pylintPath": "~/anaconda/envs/cats/bin/pylint",
    "python.linting.pylintArgs": [
        "--load-plugins",
        "pylint_django"
    ],
    "python.testing.pytestArgs": [],
    "python.testing.pytestEnabled": true
}